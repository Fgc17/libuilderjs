{
  "$schema": "https://json.schemastore.org/package",
  "name": "@ferstack/libuilderjs",
  "version": "4.0.0",
  "description": "A simple library builder",
  "author": "Fernando Coelho",
  "repository": "https://github.com/Fgc17/libuilderjs",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "src",
    "bin"
  ],
  "bin": {
    "libuilder": "dist/cmd.js"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "example": "node dist/cmd.js ./examples/.libuilderrc.js"
  },
  "dependencies": {
    "chokidar": "3.5.3",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "18.16.12",
    "prettier": "^3.2.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.14.1"
}
